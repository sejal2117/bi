  "filters": [
    {
      "name": "Last year", "description": "Show last year results only - year 2015", "cuid": "FIL_2", "path": "\\Time period\\", "state": "Active", "filter_type": "NATIVE",
      "where_expression": "Calendar_year_lookup.Yr = '2018'"
    },
    {
      "name": "This year", "description": "Show this year results only - year 2016", "cuid": "FIL_1", "path": "\\Time period\\", "state": "Active", "filter_type": "NATIVE",
      "where_expression": "Calendar_year_lookup.Yr= '2019'"
    },
    {
      "name": "Christmas period", "description": "Filter for Christmas rush period - Weeks 46 to 52 (incl. occasional week 53)", "cuid": "FIL_3", "path": "\\Time period\\", "state": "Active", "filter_type": "NATIVE",
      "where_expression": "Calendar_year_lookup.Week_In_Year BETWEEN 46 AND 53"
    },
    {
      "name": "Holiday period", "description": "Filter for weeks that contain holiday periods", "cuid": "FIL_11", "path": "\\Time period\\", "state": "Active", "filter_type": "NATIVE",
      "where_expression": "upper(Calendar_year_lookup.Holiday_Flag) = 'Y'"
    },
    {
      "name": "Sales floor size?", "description": "Prompt for sales floor group - '0-99','100-199','200-299','300-399','400-499','500+", "cuid": "FIL_6", "path": "\\Store\\", "state": "Active", "filter_type": "NATIVE",
      "where_expression": "(CASE WHEN Outlet_Lookup.Floor_space >= 0 AND Outlet_Lookup.Floor_space < 1000 THEN '0-999' WHEN Outlet_Lookup.Floor_space >= 1000 AND Outlet_Lookup.Floor_space < 2000 THEN '1000-1999' WHEN Outlet_Lookup.Floor_space >= 2000 AND Outlet_Lookup.Floor_space < 3000 THEN '2000-2999' WHEN Outlet_Lookup.Floor_space >= 3000 AND Outlet_Lookup.Floor_space < 4000 THEN '3000-3999' WHEN Outlet_Lookup.Floor_space >= 4000 AND Outlet_Lookup.Floor_space < 5000 THEN '4000-4999' ELSE '5000 +' END) IN @Prompt(Sales flloor size sqFt?)"
    },
    {
      "name": "Owned stores", "description": "Filter for wholly owned stores, rather than franchises", "cuid": "FIL_12", "path": "\\Store\\", "state": "Active", "filter_type": "NATIVE",
      "where_expression": "@Select(Store\\Store details\\Owned (y/n)) = 'Y'"
    },
    {
      "name": "Stores with long opening hours", "description": "Filter for stores with long opening hours", "cuid": "FIL_13", "path": "\\Store\\", "state": "Active", "filter_type": "NATIVE",
      "where_expression": "@Select(Store\\Store details\\Long opening hours) = 'Y'"
    },
    {
      "name": "Prompt for a line item?", "cuid": "FIL_9", "path": "\\Product\\", "state": "Active", "filter_type": "NATIVE",
      "where_expression": "Article_Color_Lookup.Family_name IN @Prompt(Choose a line to analyze?)"
    },
    {
      "name": "Which category?", "description": "Prompt for the product category", "cuid": "FIL_8", "path": "\\Product\\", "state": "Active", "filter_type": "NATIVE",
      "where_expression": "Article_Color_Lookup.Category IN @Prompt(Choose a category to analyze?)"
    },
    {
      "name": "Which product?", "description": "Prompt for the product description name.", "cuid": "FIL_10", "path": "\\Product\\", "state": "Active", "filter_type": "NATIVE",
      "where_expression": "Article_Color_Lookup.Article_label IN @Prompt(Choose the product descriptions to analyze:)"
    }
